{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "stevewitmandotcom",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "stevewitmandotcom": {
      "projectType": "application",
      "root": "apps/stevewitmandotcom",
      "sourceRoot": "apps/stevewitmandotcom/src",
      "prefix": "nx-stevewitman",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/stevewitmandotcom",
            "index": "apps/stevewitmandotcom/src/index.html",
            "main": "apps/stevewitmandotcom/src/main.ts",
            "polyfills": "apps/stevewitmandotcom/src/polyfills.ts",
            "tsConfig": "apps/stevewitmandotcom/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/stevewitmandotcom/src/favicon.ico",
              "apps/stevewitmandotcom/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/stevewitmandotcom/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/stevewitmandotcom/src/environments/environment.ts",
                  "with": "apps/stevewitmandotcom/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "stevewitmandotcom:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "stevewitmandotcom:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "stevewitmandotcom:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/stevewitmandotcom/src/**/*.ts",
              "apps/stevewitmandotcom/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/stevewitmandotcom"],
          "options": {
            "jestConfig": "apps/stevewitmandotcom/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "stevewitmandotcom-e2e": {
      "root": "apps/stevewitmandotcom-e2e",
      "sourceRoot": "apps/stevewitmandotcom-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/stevewitmandotcom-e2e/cypress.json",
            "tsConfig": "apps/stevewitmandotcom-e2e/tsconfig.e2e.json",
            "devServerTarget": "stevewitmandotcom:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "stevewitmandotcom:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/stevewitmandotcom-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "shared-material": {
      "root": "libs/shared/material",
      "sourceRoot": "libs/shared/material/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/material/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/material"],
          "options": {
            "jestConfig": "libs/shared/material/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "stevewitman-ui-main-nav": {
      "projectType": "library",
      "root": "libs/stevewitman/ui-main-nav",
      "sourceRoot": "libs/stevewitman/ui-main-nav/src",
      "prefix": "sw",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/stevewitman/ui-main-nav/src/**/*.ts",
              "libs/stevewitman/ui-main-nav/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/stevewitman/ui-main-nav"],
          "options": {
            "jestConfig": "libs/stevewitman/ui-main-nav/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ngnuggetsdotcom": {
      "projectType": "application",
      "root": "apps/ngnuggetsdotcom",
      "sourceRoot": "apps/ngnuggetsdotcom/src",
      "prefix": "nx-stevewitman",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ngnuggetsdotcom",
            "index": "apps/ngnuggetsdotcom/src/index.html",
            "main": "apps/ngnuggetsdotcom/src/main.ts",
            "polyfills": "apps/ngnuggetsdotcom/src/polyfills.ts",
            "tsConfig": "apps/ngnuggetsdotcom/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/ngnuggetsdotcom/src/favicon.ico",
              "apps/ngnuggetsdotcom/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/ngnuggetsdotcom/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ngnuggetsdotcom/src/environments/environment.ts",
                  "with": "apps/ngnuggetsdotcom/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngnuggetsdotcom:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ngnuggetsdotcom:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngnuggetsdotcom:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ngnuggetsdotcom/src/**/*.ts",
              "apps/ngnuggetsdotcom/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ngnuggetsdotcom"],
          "options": {
            "jestConfig": "apps/ngnuggetsdotcom/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ngnuggetsdotcom-e2e": {
      "root": "apps/ngnuggetsdotcom-e2e",
      "sourceRoot": "apps/ngnuggetsdotcom-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ngnuggetsdotcom-e2e/cypress.json",
            "tsConfig": "apps/ngnuggetsdotcom-e2e/tsconfig.e2e.json",
            "devServerTarget": "ngnuggetsdotcom:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ngnuggetsdotcom:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ngnuggetsdotcom-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "ngnuggets-ui-main-nav": {
      "projectType": "library",
      "root": "libs/ngnuggets/ui-main-nav",
      "sourceRoot": "libs/ngnuggets/ui-main-nav/src",
      "prefix": "ng",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ngnuggets/ui-main-nav/src/**/*.ts",
              "libs/ngnuggets/ui-main-nav/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ngnuggets/ui-main-nav"],
          "options": {
            "jestConfig": "libs/ngnuggets/ui-main-nav/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
